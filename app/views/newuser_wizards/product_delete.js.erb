var array = string.split(',');
i = jQuery.inArray('<%= @item.id %>', array)
if (parent_div == "img_box_1" || parent_div == "img_box_2" || parent_div == "img_box_3" || parent_div == "img_box_4" || parent_div == "img_box_5" || parent_div == "img_box_6")
{
$("#" + parent_div).html("<%= escape_javascript(render "default_image")%>")
}
