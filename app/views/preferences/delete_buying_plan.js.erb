<%if params[:type] == "complete" %>
$("#owned_item").html("<%= escape_javascript(render 'preferences/owned_item_select', {:item => @item}) %>");
$(".Closebut").show();
$("#owned_item").bPopup({
    closeClass:'Closebut',
    modalClose: false,
    position: [(screen.width / 2) - (920/2), $(window).scrollTop() + 120],
    follow: [false, false] //x, y
 });
<%else%> 
  window.location.href = "/<%=current_user.username%>?follow=Preferences";
<%end%>
