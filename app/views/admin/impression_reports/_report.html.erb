
<center><h2>Report From <%= @start_date.to_date.strftime("%d-%m-%Y") %> To <%= @end_date.to_date.strftime("%d-%m-%Y") %></h2></center>
<br />

<h2> Summary </h2>

<br/>

<table border="0" width="100%" cellspacing="0" cellpadding="0">
<tr>
  <th class="header_compare"  > Total Impressions </th>
  
  
 </tr>
   <tr>
    <td class="bdb_gray1"  style="width:100px;text-align:center;border-bottom: 1px solid #808080;"> <%= @impressionscount %> </td>
     
  </tr>

 </table> 

<br/><br/>

<%if @impressionscount > 0 %>

<h2> Impressions </h2>
<br/>
<table border="0" width="100%" cellspacing="0" cellpadding="0">
<tr>
  <th class="header_compare"  style="width:100px;"> Impression Time </th>
  <th class="header_compare" width="600px"> Hosted Page </th>
  
 </tr>
 <%@impressions.each do |im | %>
   <tr>
    <td class="bdb_gray1"  style="width:100px;text-align:center;border-bottom: 1px solid #808080;"><%=  im.impression_time.to_datetime.strftime("%d-%b-%Y %I:%M %p" ) %> </td>
     <td class="bdb_gray1" width="600px" style="text-align:center;border-bottom: 1px solid #808080;"><%= im.hosted_site_url %> </td>      
     
  </tr>
<%end%>  
 </table> 

<br/><br/>
 <%= will_paginate @impressions %>
<%end%>


