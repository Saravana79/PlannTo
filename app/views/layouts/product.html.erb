<script type="text/javascript" src="/javascripts/layout.js"></script>
</head>
<body marginheight="0" topmargin="0">
<%= render 'shared/header_links' %>
<div id="dialog-form" title="Create new user">
    <div id="login">
       <%= render "shared/login_dialog" %>
          <div class="Dontsignup">Don't have an account. <%= link_to "Sign up now" ,"#", :onclick =>"$('#sign_up_dialog_1').show();$('#login').hide();" %></div>
   </div>
  <div id="sign_up_dialog_1" style="display:none;">

       <%= render "/shared/signup_dialog" %>
<div class="Dontsignup">Do you have an account. <%= link_to "Sign in now" ,"#", :onclick =>"$('#sign_up_dialog_1').hide();$('#login').show();" %></div>
</div>
  <%= render 'shared/show_hide_login_form', :item => "" %>
</div>
<div id="maincontainer" class="maincontainerCss">
<div class="breadcrumpbtnDiv">
<%= yield %>
<!-- This will go into main layout -->
</div>
<br />


<div class="footerdiv">
<%= render 'shared/footer' %>
  <div style="display:none;">
  <%= render "shared/login_dialog", :item => @item %>
  </div>
</div>
<div id="sign_up_dialog" style="display:none">
    <div id="login1" style="display:none;">
       <%= render "shared/login_dialog" %>
        <div class="Dontsignup">Don't have an account. <%= link_to "Sign up now" ,"#", :onclick =>"$('#sign_up_dialog2').show();$('#login1').hide();" %></div>
   </div>
   
  <div id="sign_up_dialog2">

       <%= render "/shared/signup_dialog" %>
<div class="Dontsignup">Do you have an account. <%= link_to "Sign in now" ,"#", :onclick =>"$('#sign_up_dialog2').hide();$('#login1').show();" %></div>

</div></div>
