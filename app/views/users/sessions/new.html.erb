<script>
if(window.opener) {
    window.close()
 }
 </script>
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
   <div style="height:500px;left:center;width: 50%; margin: 0 auto;">
   <div style="width:300px">
  
    <% if @rescue == "true" %>
      <%= render  '/shared/login_popup_message',:type => 'login-devise'%>
   <%end%> 
    <%= render "shared/login_fields", {:f => f} %>
    </div>
   </div>
  
<% end %>

  
