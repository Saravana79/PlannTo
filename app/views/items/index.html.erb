<div id="comment-notice"></div>

<div id="compare-list" class="productcontentmain" style="display:none">
  <span id="compare-items"><%= link_to(("<span>Compare</span>").html_safe, compare_items_path + '?ids=0' , :class => "imageButtonAsk") %></span>
</div>

<%= @items.count%>

<br>
<%@items.each do |item|%>

    <div class="productcontentmain">
      <div>
        <img class="productimage" src="/assets/Fiat_Grande_Punto__175.jpg"/>
      </div>
      <div>
        <span class="producttitle"><%= item.name %></span>
        <%#*</div>%>
        <div class="productprice">
          <%= item.get_price_info('Price') %>
        </div>
        <div class="productreview">
          <img src="/assets/star2.gif"/><img src="/assets/star2.gif"/><img src="/assets/star2.gif"/><img src="/assets/star2.gif"/><img src="/assets/white.gif"/>Ratings
          <a class="feedlink">(100 Reviews)</a>
        </div>
        <div class="productspecification">

          <% item.item_attributes.select("attribute_values.value, name, unit_of_measure, category_name").limit(5).each do |item_attribute| %>
            <%= item_attribute.unit_of_measure %> <%= item_attribute.value %> ( <%= item_attribute.name %> ),
          <% end %>

        </div>
        <span><%= check_box_tag("add_to_compare_#{item.id()}", nil,false ,:checked => nil,
                                :class => "add-compare", :rel =>item.id())%>
              <span>Add to Compare</span>
              
        </span>
      </div>
    </div>
    <div class="compare-view" style="display:none; width:20%; float:left;">
      name <%= item.name %>
      <div>
      <img alt="car" class="mediumimage" src="/assets/MarutiRitz_100x60.jpg"/>

      <div>
        Maruti Ritz
      </div>
      <div>
        3 Star Rating
      </div>
      <div>
        <a class="feedlink">Follow</a>
      </div>
      <div>
        Rahul, Deepak and 100 other people owns this.
        <a class="remove-compare" href="javascript:void(0);"  onclick="javascript:removeCompare(this);" style="float:right; color:#5BAEF4;" rel="<%=item.id()%>">Remove</a>
      </div>
      <hr/>
    </div>
    </div>

<% end %>
<br>
