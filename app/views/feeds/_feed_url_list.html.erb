  <% @feed_urls.each_with_index do |feed, inx| %>
    <div style="height:20px;overflow: hidden;position: relative;" class='each_row'>
     <div style="width:200px;float:left;"><%= feed.source.blank? ? "-- blank -- " : feed.source %></div>
     <div style="width:100px;float:left;" class="mark_as_list">
       <a href="javascript:void(0)" id="future" class="mark_as_button"><%= image_tag "/assets/star-on.png", :alt => "mark as future", :title => "mark as future" %></a>
       <a href="javascript:void(0)" id="invalid" class="mark_as_button"><%= image_tag "/assets/invalid.png", :size => "16x16", :alt => "mark as invalid", :title => "mark as invalid" %></a>
     </div>
      <div style="width:650px;float:left;"><span class="feed_title" id="<%= feed.id %>"><%= feed.title.blank? ? feed.url : feed.title.truncate(70) %></span> </div>
      <div style="width:50px;float:left;"><%= feed.category %></div>     
      
      <div class="feed_explain"> <td colspan="5">  </div> 

  </div>
  <div class="display" id="feed_display_<%= feed.id %>" style="overflow: hidden;position: relative;border:1px solid #CFCFCF"></div>
  </div>
  <% end %>
