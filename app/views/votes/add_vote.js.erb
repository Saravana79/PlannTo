$("#votersResult<%=@voteable.id%>").text(<%=@voteable.voters_who_voted.size%>);
<%class_name = get_voting_class_name(current_user, @voteable) %>
$("#dislikeVote<%=@voteable.id%>")[0].className = '';
$("#dislikeVote<%=@voteable.id%>").addClass("<%=class_name[:negative]%>");
$("#likeVote<%=@voteable.id%>")[0].className = '';
$("#likeVote<%=@voteable.id%>").addClass("<%=class_name[:positive]%>");





