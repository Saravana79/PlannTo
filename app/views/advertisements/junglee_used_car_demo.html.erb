<html>
<head>
  <link href='http://fonts.googleapis.com/css?family=ABeeZee' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Oswald' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Roboto Slab' rel='stylesheet' type='text/css'>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
  <style>
      html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video {
          margin: 0;
          padding: 0;
          border: 0;
      }

      table {
          border-collapse: collapse;
          border-spacing: 0;
      }

      .ad > span {
          font-size: 14px;
      }
  </style>
</head>
<body>
<div class="advertisements">
  <h1>Junglee Car Demo</h1>
  <div class="demo_form row">
    <div class="search_form">
      <%= form_for :search, :url => "/advertisements/junglee_used_car_demo", :html => {:method => 'get'} do |search_form| %>
          City : <%= select_tag :city_id, options_for_select([["Bangalore", "29574"], ["New Delhi", "29758"], ["Chennai", "29673"], ["Hyderabad", "29461"], ["Mumbai", "29858"]], params[:city_id]) %>
          Car : <%= select_tag :car_ids, options_for_select([["Maruti Swift", "581,582,583,584,585,586"], ["Hyundai i10", "332,333,334,335,336,337,338,339,340,341"], ["Honda City", "12089,12090,12091,12092,12093,12094,12095,12096,12097,12098,12099,12100,12101"]], params[:car_ids]) %>
          <%= search_form.submit "Go" %>
      <% end %>
    </div>
  </div>

  <div class="ad_list">
    <div class="row">
      <div class="ad_300_250">
        <span>300x250</span>
        <iframe id="plannto_ad_frame_300" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=300*250&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="300px" height="250px" style="border: medium none;;"></iframe>
      </div>

      <br clear="both"/>
      <div class="ad_468_60">
        <span>468x60</span>
        <iframe id="plannto_ad_frame_468" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=468*60&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="468px" height="60px" style="border: medium none;;"></iframe>
      </div>

      <br clear="both"/>
    </div>
    <div class="row">
      <div class="row">
        <div class="ad_728_90">
          <span>728x90</span>
          <iframe id="plannto_ad_frame_728" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=728*90&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="728px" height="90px" style="border: medium none;;"></iframe>
        </div>

        <br clear="both"/>
      </div>
    </div>

    <br clear="both"/>
    <div class="row" style="clear: both; padding-top: 44px;">
      <div class="ad_300_600">
        <span>300x600</span>
        <iframe id="plannto_ad_frame_300_600" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=300*600&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="300px" height="600px" style="border: medium none;;"></iframe>
      </div>

      <br clear="both"/>
      <div class="ad_120_600">
        <span>120x600</span>
        <iframe id="plannto_ad_frame_120" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=120*600&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="120px" height="600px" style="border: medium none;;"></iframe>
      </div>
    </div>

    <br clear="both"/>
    <div class="row" style="clear: both; padding-top: 44px;">
      <div class="ad_336_280">
        <span>336x280</span>
        <iframe id="plannto_ad_frame_336_280" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=336*280&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="336px" height="280px" style="border: medium none;;"></iframe>
      </div>

      <br clear="both"/>
      <div class="ad_250_250">
        <span>250x250</span>
        <iframe id="plannto_ad_frame_250_250" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=250*250&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="250px" height="250px" style="border: medium none;;"></iframe>
      </div>

      <br clear="both"/>
      <div class="ad_200_200">
        <span>200x200</span>
        <iframe id="plannto_ad_frame_200_200" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=200*200&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="200px" height="200px" style="border: medium none;;"></iframe>
      </div>
    </div>

    <br clear="both"/>
    <div class="row" style="clear: both; padding-top: 44px;">
      <div class="ad_160_600">
        <span>160x600</span>
        <iframe id="plannto_ad_frame_160_600" src="<%= "#{configatron.hostname}/advertisments/show_ads?item_id="+"#{params[:item_id]}"+"&ads_id="+"#{params[:ads_id]}"+"&size=160*600&more_vendors="+"#{params[:more_vendors]}"+"&ref_url="+"#{params[:ref_url]}"+"&ad_as_widget=true&is_test=#{params[:is_test]}&page_type=#{params[:page_type]}"; %>" width="160px" height="600px" style="border: medium none;;"></iframe>
      </div>
    </div>
  </div>
</div>
</body>
</html>


