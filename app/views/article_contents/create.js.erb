
<% if @article.errors.any? %>
  $("#article_errors").html("<%= errors_for(@article) %>");
  $("#article_errors").show(300);	
<% else %>
  <% if @external.blank? %>
    $("#article_errors").hide();
    window.location = "/cars/<%= @item_id %>" ; 
  <% else %>
    window.open('', '_self', '');
    window.close();
  <% end %>
<% end %>
