
<% if @article.errors.any? %>
  $("#article_errors").html("<%= errors_for(@article) %>");
  $("#article_errors").show(300);	
<% else %>
  <% if @external.blank? %>
    $("#article_errors").hide();
   $('#article_content_url').val('');
   $('#article_content_url').keyup();
   $("#newContentList").html('<%= escape_javascript(render_content_type(@article)) %>');
  <% else %>   
    window.open('', '_self', '');
    window.close();
  <% end %>
<% end %>
