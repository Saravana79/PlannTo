<%if @duplicate_url == "true"%>
  <script>
     alert('This article is already shared in PlannTo.com');
  </script>
<%end%>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>	
<script type="text/javascript" src="/javascripts/jquery-ui-1.8.14.custom/js/jquery-ui-1.8.14.custom.min.js"></script>
<link type="text/css" href="/javascripts/jquery-ui-1.8.14.custom/css/ui-lightness/jquery-ui-1.8.16.custom.css" rel="stylesheet"/>
<%= javascript_include_tag 'textTagger' %>
<%= javascript_include_tag 'application' %>
<%if current_user %>
<%= render :partial => "article_contents/article_share", :locals => {:content_category => true} %>
<%#= render :partial => 'article_contents/article_share', :locals => {:content_category => true} %>
<%else%>
<%= form_for(@user, :as => :user, :url => session_path("user"), :method => :post) do |f| %>
<div style="height:500px;left:center;width: 50%; margin: 0 auto;">
   <div style="width:300px">
  
    <% if @rescue == "true" %>
      <%= render  '/shared/login_popup_message',:type => 'login-devise'%>
   <%end%> 
    <%= render "shared/login_fields", {:f => f} %>
    </div>
   </div>
 <%end%>
<%end%>
