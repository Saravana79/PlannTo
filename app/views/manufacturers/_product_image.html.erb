<span class="img_product">
<%- @item.car_groups.each do |item| %>

          <%= link_to({:controller => item.class.name.underscore.pluralize, :action => "show", :id => item.id}) do %>
            <%= image_tag(get_image_url(item, item.class.name), :title => item.name, :class => "productimage", :width => "205", :height => "124" ) %>

        <% end %>

      <%- end %>
</span>
